{
    "name": "your-vendor/laravel-sage",
    "description": "A simple and elegant AI assistant package for Laravel applications",
    "keywords": [
        "laravel",
        "ai",
        "chatbot",
        "assistant",
        "openai",
        "anthropic",
        "gpt",
        "claude"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelSage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSage\\SageServiceProvider"
            ],
            "aliases": {
                "Sage": "LaravelSage\\Facades\\Sage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
