{
    "name": "falahatiali/homa",
    "description": "Homa - The legendary bird that brings AI wisdom to Laravel. A simple and elegant AI assistant package.",
    "keywords": [
        "laravel",
        "ai",
        "chatbot",
        "assistant",
        "openai",
        "anthropic",
        "gpt",
        "claude",
        "homa",
        "persian"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fali",
            "email": "falaahatiali@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "openai-php/laravel": "^0.16.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "Homa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Homa\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Homa\\HomaServiceProvider"
            ],
            "aliases": {
                "Homa": "Homa\\Facades\\Homa"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
